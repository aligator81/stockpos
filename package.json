{
  "name": "stock-pos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node src/server/index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "migrate": "node --loader ts-node/esm src/scripts/migrate.ts",
    "migrate:up": "node --loader ts-node/esm src/scripts/migrate.ts up",
    "migrate:down": "node --loader ts-node/esm src/scripts/migrate.ts down",
    "migrate:create": "node-pg-migrate create"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@hello-pangea/dnd": "^17.0.0",
    "@react-pdf/renderer": "^4.1.5",
    "@stripe/stripe-js": "^5.2.0",
    "@types/pg": "^8.11.10",
    "@types/react-datepicker": "^6.2.0",
    "@types/uuid": "^10.0.0",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.7",
    "emailjs-com": "^3.2.0",
    "express": "^4.18.3",
    "lucide-react": "^0.294.0",
    "node-pg-migrate": "^7.8.0",
    "pg": "^8.13.1",
    "react": "^18.2.0",
    "react-barcode": "^1.5.3",
    "react-barcode-reader": "^0.0.2",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.4.0",
    "react-qr-code": "^2.0.15",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "react-router-dom": "^6.20.0",
    "react-to-print": "^3.0.2",
    "recharts": "^2.13.3",
    "tailwind-merge": "^2.5.5",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.4"
  }
}
